{
    "name": "agenter-labs/illuminate-iam",
    "description": "Auth checker for Laravel",
    "keywords": [
        "laravel",
        "auth",
        "checker"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulla",
            "email": "dev@agenter.com",
            "homepage": "https://agenter.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "agenter-labs/illuminate-database": "^1.0",
        "agenter-labs/illuminate-uid": "^1.0",
        "agenter-labs/illuminate-gate": "^1.0"
    },
    "require-dev": {
        "laravel/lumen-framework": "^9.0",
        "fakerphp/faker": "^1.16",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "AgenterLab\\IAM\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenterLab\\IAM\\IamServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Database\\Factories\\": "database/factories/"
        }
    }
}
